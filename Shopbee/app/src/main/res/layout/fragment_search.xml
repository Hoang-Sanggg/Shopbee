<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginBottom="10dp"
        android:background="#FF5733"
        android:orientation="horizontal">
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:text="Câu Nói Truyền Động Lực"
            android:padding="15dp"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#EAEDED"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:text="Đồng hồ dành cho báo thủ"
            android:padding="15dp"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#EAEDED"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:text="Áo Đại Bàng tung cánh"
            android:padding="15dp"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#EAEDED"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:text="Quần Chiến Thần"
            android:padding="15dp"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#EAEDED"/>
        <androidx.appcompat.widget.AppCompatButton
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:textSize="15dp"
            android:text="Xoá Lịch Sử Tìm Kiếm"
            android:background="@color/white"
            android:textColor="#D6DBDF  "/>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="#EAEDED"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="52dp"
        android:padding="15dp"
        android:text="Search Suggestions"
        android:textColor="@color/black"
        android:textStyle="bold" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <LinearLayout
            android:background="@drawable/rd_search"
            android:layout_width="205dp"
            android:layout_height="wrap_content"
            android:padding="15dp"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:src="@drawable/sp1_search"
                />
            <TextView
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:text="cau noi truyen dong luc"
                android:padding="5dp"
                android:layout_marginLeft="5dp"/>

        </LinearLayout>
        <LinearLayout
            android:background="@drawable/rd_search"
            android:layout_width="210dp"
            android:layout_height="wrap_content"
